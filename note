
con este corrio local

spring.datasource.url=jdbc:sqlite:./data/sistema_examenes.db
spring.datasource.driver-class-name=org.sqlite.JDBC


spring.jpa.database-platform=com.sistema.examenes.configuraciones.SQLiteDialect


spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true
spring.jpa.properties.hibernate.format_sql=true


logging.level.org.hibernate.SQL=DEBUG
logging.level.org.hibernate.type.descriptor.sql.BasicBinder=TRACE


debug=true

logging.level.org.springframework.web=DEBUG
logging.level.org.springframework.boot.autoconfigure.web=DEBUG
logging.level.com.sistema.examenes=DEBUG

